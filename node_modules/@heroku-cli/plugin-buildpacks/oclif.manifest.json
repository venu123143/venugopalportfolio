      "strict": true,
      "pluginName": "@heroku-cli/plugin-buildpacks",
      "pluginAlias": "@heroku-cli/plugin-buildpacks",
      "pluginType": "core",
      "aliases": [],
      "flags": {
        "app": {
          "name": "app",
          "type": "option",
          "char": "a",
          "description": "app to run command against",
          "required": true,
          "multiple": false
        },
        "remote": {
          "name": "remote",
          "type": "option",
          "char": "r",
          "description": "git remote of app to use",
          "multiple": false
        },
        "index": {
          "name": "index",
          "type": "option",
          "char": "i",
          "description": "the 1-based index of the URL in the list of URLs",
          "multiple": false
        }
      },
      "args": {
